{
  "name": "portfolio-robert-wawrzyniak",
  "version": "2.0.0",
  "description": "My website powered by Gatsby",
  "main": "src/pages/index.js",
  "scripts": {
    "build": "cleandir ./.cache && gatsby build --no-uglify",
    "develop": "cleandir ./.cache && gatsby develop",
    "start": "npm run develop",
    "lint": "eslint ./src",
    "setup": "node ./bin/setup.js",
    "fmt": "prettier --write \"{*,src/**/*}.{js,json,md,yml,css}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thuringia/website.git"
  },
  "keywords": [
    "gatsby",
    "starter",
    "contentful",
    "PWA",
    "portfolio"
  ],
  "author": "Robert Wawrzynian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thuringia/website/issues"
  },
  "homepage": "https://wawrob.xyz",
  "dependencies": {
    "clean-dir": "^1.0.3",
    "contentful": "^7.0.3",
    "dotenv": "^6.0.0",
    "envfile": "^3.0.0",
    "gatsby": "^2.0.7",
    "gatsby-plugin-google-analytics": "^2.0.9",
    "gatsby-plugin-google-fonts": "^0.0.4",
    "gatsby-plugin-manifest": "^2.0.2",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-styled-components": "^3.0.0",
    "gatsby-source-contentful": "^2.0.1",
    "gatsby-source-medium": "^2.0.0",
    "gatsby-transformer-remark": "^2.1.3",
    "inquirer": "^6.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-fontawesome": "^1.6.1",
    "react-headroom": "^2.2.4",
    "react-helmet": "^5.2.0",
    "react-markdown": "^4.0.3",
    "react-reveal": "^1.2.2",
    "react-scroll-section": "^1.3.0",
    "react-text-loop": "^2.0.1",
    "react-tippy": "^1.2.2",
    "rebass": "^3.0.0",
    "styled-components": "^4.0.2"
  },
  "devDependencies": {
    "chalk": "^2.4.1",
    "contentful-import": "^7.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.0",
    "prettier": "^2.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,yml}": [
      "prettier --write"
    ]
  }
}
